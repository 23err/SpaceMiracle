<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spacemiracle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/ThemeRed">
        <activity android:name=".AnimatedVectorDrawableActivity"></activity>
        <activity android:name=".ConstraintSetActivity" />
        <activity android:name=".StateListAnimatorActivity" />
        <activity android:name=".AnimationObjectActivity" />
        <activity android:name=".MixingActivity" />
        <activity android:name=".AnimationPathActivity" />
        <activity android:name=".ChangeImageTransformActivity" />
        <activity android:name=".AnimationActivity" />
        <activity android:name=".MotionActivity" />
        <activity
            android:name=".SwipeBehaviorActivity"
            android:label="@string/title_activity_swipe_behavior"
            android:theme="@style/ThemeRed.NoActionBar" />
        <activity android:name=".CoordinatorLayoutActivity" />
        <activity android:name=".ThirdActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>